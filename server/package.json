{
  "name": "dear-blueno-server",
  "version": "0.1.0",
  "description": "Backend server for the Dear Blueno web app",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx ts-node src/index.ts",
    "build": "tsc",
    "test": "jest --testPathPattern=src/tests/integration --runInBand",
    "test:parallel": "jest --testPathPattern=src/tests/integration",
    "test:build": "npm run build && jest --testPathPattern=dist/tests/integration",
    "test:e2e": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint --fix . && prettier -w ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dear-Blueno/dearblueno.git"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "keywords": [
    "blueno",
    "brown",
    "university"
  ],
  "author": "Dear Blueno Team",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Dear-Blueno/dearblueno/issues"
  },
  "homepage": "https://dearblueno.net",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/google-spreadsheet": "^3.1.5",
    "@types/jest": "^27.0.3",
    "@types/node-cron": "^3.0.0",
    "@types/passport": "^1.0.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "google-spreadsheet": "^3.2.0",
    "log4js": "^6.4.1",
    "mongoose": "^6.0.13",
    "node-cron": "^3.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "response-time": "^2.3.2",
    "tslib": "^2.3.1"
  }
}
